{
  "name": "ivao-whazzup",
  "version": "1.1.2",
  "description": "A lib for getting IVAO Whazzup data",
  "author": "Joao Pedro Henrique <joao.pedro.hsd@gmail.com>",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "coveralls": "jest --coverage && cat ./__tests__/coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "github:jpedroh/ivao-whazzup"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "request": "^2.87.0",
    "tslint": "^5.11.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "@types/node": "^10.5.2",
    "@types/request": "^2.47.1",
    "coveralls": "^3.0.3",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
